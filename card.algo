PROCEDURE cartes-rami (VAR arr : ARRAY_OF INTEGER)
VAR
    i , j , tmp : INTEGER .
BEGIN
    FOR i FROM 1 TO arr.length-1 STEP step 1  DO
        tmp := arr [i]
        j := i - 1 ;
        WHILE (j >= 0 AND arr[j] > tmp) DO
            arr[j+1] := arr[j] ;
            j:= j-1 ;
        END_WHILE
        arr[j+1] = tmp ;
    END_FOR
END